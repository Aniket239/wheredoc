<table>
    <thead>
        <tr>
            <td>Company Name</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        <% @companies.each do |company|%>
        <tr>
            <td><%= company.name%></td>
            <td><%= link_to 'Edit', company_edit_path(company)%></td>
            <td><%= link_to 'Delete', company_delete_path(company)%></td>
        </tr>
        <% end%>
    </tbody>
</table>
<%= link_to 'Register New Company', company_new_path%>
